<core:FragmentDefinition
    xmlns="sap.ui.comp.valuehelpdialog"
    xmlns:m="sap.m"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:core="sap.ui.core"
>
    <ValueHelpDialog
        supportMultiselect="false"
        title="{i18n>account}"
        ok=".valueHelp.onActionVHAccountList"
        cancel=".valueHelp.onActionVHAccountList"
        supportRanges="false"
        key="CodeId"
        descriptionKey="CodeName"
    >
        <filterBar>
            <fb:FilterBar
                advancedMode="false"
                search=".valueHelp.onActionVHAccountList"
                isRunningInValueHelpDialog="true"
            />
        </filterBar>
    </ValueHelpDialog>
</core:FragmentDefinition>
